<div class="row">
  <div class="col-12">
    <h2>Follower Users</h2>
    <table class="table text-center">
      <thead>
        <tr>
          <th>image</th>
          <th>name</th>
          <th></th>
          <th></th>
          <th></th>
        </tr>
      </thead>
      <tbody>
        <% @users.each do |user| %>
          <tr>
            <td><%= image_tag user.get_profile_image(50,50) %></td>
            <td><%= user.name %></td>
            <td>フォロー数: <%= user.followeds.count %></td>
            <td>フォロワー数: <%= user.followers.count %></td>
            <td><%= link_to "Show", user_path(user) %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>